<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gnome-shell-extensions">

  <schema path="/org/gnome/shell/extensions/google-calculator/" id="org.gnome.shell.extensions.google-calculator">

    <key type="as" name="show-calculator">
      <default><![CDATA[['<Super>C']]]></default>
    </key>

    <key type="s" name="google-suggestions-url">
        <default><![CDATA['https://suggestqueries.google.com/complete/search?client=chrome&q=%s']]></default>
    </key>

    <key type="s" name="google-currency-url">
        <default><![CDATA['https://www.google.com/finance/converter?a=%s&from=%s&to=%s']]></default>
    </key>

    <key type="i" name="timeout">
        <default>400</default>
    </key>

    <key type="as" name="history">
        <default>[]</default>
    </key>

    <key type="i" name="history-limit">
        <default>50</default>
    </key>

    <key type="b" name="indicator">
        <default>false</default>
    </key>

    <key type="i" name="dialog-width-percents">
        <default>30</default>
    </key>

    <key type="i" name="dialog-height-percents">
        <default>60</default>
    </key>

    <key type="b" name="enable-effects">
        <default>true</default>
    </key>

    <key type="s" name="currency-default-from">
        <default>'USD'</default>
    </key>

    <key type="s" name="currency-default-to">
        <default>'RUB'</default>
    </key>

    <key type="s" name="currency-default-key">
        <default>'$'</default>
    </key>

  </schema>
</schemalist>
